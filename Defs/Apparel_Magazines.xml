	<!-- Preindustrial -->
	
		<!-- Small -->
	<ThingDef ParentName="ApparelMakeableBase">
		<defName>Apparel_AmmoPackPreindustrialSmall</defName>
		<label>small preindustrial pack</label>
		<description>A small pack for carrying preindustrial ammunition. It has enough space for 22 ammo.</description>    
		<thingClass>Ammunition.Things.Magazine</thingClass>
		<graphicData>
			<texPath>MagPreindustrial/AmmoPackPreindustrialSmall</texPath>
			<shaderType>CutoutComplex</shaderType>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<techLevel>Medieval</techLevel>
		<statBases>
			<Mass>2</Mass>
			<WorkToMake>1000</WorkToMake>
			<Flammability>0.6</Flammability>
			<EquipDelay>2</EquipDelay>
		</statBases>		 
		<costStuffCount>15</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>   
		<tradeability>All</tradeability>
		<thingCategories>
			<li>AmmoPacks</li>
		</thingCategories>		
		<apparel>
			<bodyPartGroups>
				<li>Legs</li>
			</bodyPartGroups>
			<layers>
				<li>Kit</li>
			</layers>
			<tags>
				<li>Medieval</li>
			</tags>
		</apparel>
		<recipeMaker>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
		</recipeMaker>  
		<comps>			
			<li Class="Ammunition.Comps.CompProps_Magazine">
				<AmmoCapacity>22</AmmoCapacity>
				<AmmoDef>AmmoPreindustrial</AmmoDef>
				<Icon>MagPreindustrial/AmmoPackPreindustrialSmall</Icon>
			</li>
		</comps>
	</ThingDef>
